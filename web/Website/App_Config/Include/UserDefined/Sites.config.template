				  enableTracking="true" allowDebug="true"
          enablePreview="true" enableWebEdit="true"
          enableDebugger="true" disableClientData="false">
        #if ($Env.DeliveryOnly == "true")
        <patch:attribute name="enablePreview">false</patch:attribute>
        <patch:attribute name="enableWebEdit">false</patch:attribute>
        #end
      </site>
