<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <sites>
      <site patch:before="site[@name='website']" database="web" domain="extranet" virtualFolder="/" physicalFolder="/"
				  name="InVivo"
				  dictionaryDomain="InVivoDictionary"
				  hostName="${Site.Domain.InVivo}" targetHostName="${Site.Domain.InVivo}"
