<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <processors>
      <saveUI>
        <processor mode="on" type="Informa.Library.CustomSitecore.Pipelines.Article.SaveTitle, Informa.Library" patch:after="processor[@type='Sitecore.Pipelines.Save.Save, Sitecore.Kernel']"/>
      </saveUI>
    </processors>
    <events>
      <event name="item:added">
        <handler type="Informa.Library.CustomSitecore.Events.AddArticleFromCMS, Informa.Library" method="OnArticleAddedFromCMS" />
      </event>
    </events>
    <clientscripts>
      <htmleditor>
        <script src="/sitecore/shell/Controls/Rich Text Editor/RTECustomization.js" language="javascript" key="customJs" />
      </htmleditor>
    </clientscripts>
  </sitecore>
</configuration>
