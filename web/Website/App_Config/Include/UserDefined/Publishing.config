      <event name="publish:begin">
        <handler type="Informa.Library.Publishing.Events.BeginPublishItemProcessing, Informa.Library" method="Process" />
      </event>
      <event name="publish:itemProcessing">
        <handler type="Informa.Library.Publishing.Events.ActualPublishDateArticlePublishItemProcessing, Informa.Library" method="Process" />
      </event>
    </events>
  </sitecore>
</configuration>
