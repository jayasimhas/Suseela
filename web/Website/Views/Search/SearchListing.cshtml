@model dynamic

<section class="search-results" ng-controller="searchResultsController as results">

   

    <!-- start Media Object - Default (no width) -->
    <div class="media-object" ng-repeat="doc in results.docs">

        <div class="media-object__body">
            <b>{{ doc.publicationTitle }} </b>
            <h4 class="media-object__heading">
                <a ng-href="{{ doc.url }}" title="Page Title">
                    {{ doc.title }}
                </a>
            </h4>
            <ul class="media-object__meta">
                <li class="media-object__meta-author">by {{ doc.byline }}</li>
                <li class="media-object__meta-pubdate">
                    <time datetime="2015-01-15">{{doc.searchDate | date: 'longDate' }}</time>
                </li>
                <li class="media-object__meta-pubdate">
                    | {{doc.contentType }}
                </li>
            </ul>
            <p class="media-object__description">{{ doc.summary }}</p>
        </div>

        <span ng-repeat="tax in doc.searchDisplayTaxonomy.links">
            <a  href="{{tax.url}}">
                {{tax.title}}
            </a><span ng-if="!$last">, </span>
        </span>
    </div>

    
    <!-- end Media Object - Default (no width) -->
</section>