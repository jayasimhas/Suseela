@model Velir.Search.Models.I_Listing_Configuration

<script src="/velir/vendor/bower_components/lodash/lodash.js"></script>
<script src="/dist/js/angular-1.5.1.min.js"></script>
<script src="/dist/js/angular-animate-1.5.1.min.js"></script>
<script src="/dist/js/angular-sanitize-1.5.1.min.js"></script>
<script src="/velir/dist/velir.search.angular.js"></script>

@* Concatenates our Angular files for Informa Search. Controllers, etc. *@
<script src="/dist/js/search.js"></script>

<script>
    (function (angular) {

        var informaSearchApp = angular.module('informaSearchApp');

        informaSearchApp.config(function (searchServiceProvider) {
            searchServiceProvider.setUrl('@Model.Base_Endpoint_Url?@Velir.Search.Core.Reference.SiteSettings.QueryString.PageIdKey=@Model._Id');
        });

    })(angular);
</script>
