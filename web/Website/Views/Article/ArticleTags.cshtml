@using System.Collections
@model IEnumerable<Informa.Models.FactoryInterface.ILinkable>
<div class="article-prologue__tags-bar">
    Tags:
    <div class="bar-separated-link-list">
        @foreach (var tag in Model.Take(3))
        {
            <a href="@tag.LinkableUrl">@tag.LinkableText</a>
        }
    </div>
</div>

