@model Informa.Library.Utilities.DataModels.TreeNode<string>
<li class="article-topics__li">
    <a href="/companies/_id@(Model.Note)">@Model.Value</a>
    @if (Model.Children.Any())
    {
        <ul>
            @foreach (var child in Model.Children)
            {
                @Html.Partial("~/Views/CompaniesAndDeals/SubsidiariesTree.cshtml", child)
            }
        </ul>
    }
</li>    