
@model  Informa.Web.ViewModels.FinanceCompanies.CompanyAddressViewModel


<div style="border:2px solid Grey;">
    <div>
        @Model.CompanyID
    </div>

    <div>
        @Model.CompanyName
    </div>

    <div>
        @Model.CompanyAddress
    </div>
    <div>
        <img src="@Model.CompanyLogo" style="height:50px;" />
    </div>

    <div>
        <a href="@Model.CompanyUrl">@Model.CompanyUrl</a>
    </div>

    <div>
        @Model.CompanyIntroductionText
    </div>
</div>