<?xml version="1.0" encoding="UTF-8"?>

<suite name="Informa Regression Test Suite" verbose="10" parallel="none">



    <!--SCRIP TESTCASES ON CHROME, FIREFOX, IE-11, MOBILE-->

    <test name="SCRIP Tests on Chrome">
        <parameter name="browser" value="Chrome"/>
        <parameter name="environment" value="Stage.Scrip"/>

        <classes>
            <class name="com.velir.Homepage"/>
            <class name="com.velir.SearchBoxOnResult"/>
            <class name="com.velir.EmailResults"/>
            <class name="com.velir.Registration"/>
            <class name="com.velir.ForgotPassword"/>
            <class name="com.velir.MyAccount"/>
            <class name="com.velir.ManageMyInformation"/>
            <class name="com.velir.RSSFeed"/>
            <class name="com.velir.XMLrecord"/>
        </classes>
    </test>

    <test name="SCRIP Tests on Firefox">
        <parameter name="browser" value="FF"/>
        <parameter name="environment" value="Stage.Scrip"/>

        <classes>
            <class name="com.velir.Homepage"/>
            <class name="com.velir.SearchBoxOnResult"/>
            <class name="com.velir.EmailResults"/>
            <class name="com.velir.Registration"/>
            <class name="com.velir.ForgotPassword"/>
            <class name="com.velir.MyAccount"/>
            <class name="com.velir.ManageMyInformation"/>
            <class name="com.velir.RSSFeed">
                <methods>
                    <exclude name="rssFeedPublication"/>
                </methods>
            </class>
            <class name="com.velir.XMLrecord"/>
        </classes>
    </test>

